<button><%= link_to "Home Page", "/" %></button>
 | | 

<% if logged_in? %>
    <button><%= link_to "Profile Page", user_path(current_user) %></button>
     | | 
    <button><%= link_to "Edit Profile", edit_user_path(current_user) %></button>
     | | 
    <button><%= link_to "Logout", "/logout", method: 'post' %></button>
    <% if current_user.admin %>
         | | 
        <button><%= link_to "Admin Resources", '/admin_resources', method: 'get' %></button>
    <% end %>
<% else %>
    <button><%= link_to "Sign in here", "/login" %></button>
     | | 
    <button><%= link_to "Sign up here", new_user_path %></button>
     | | 
    <button><%= link_to "Enter as Visitor", visitors_path %></button>
<% end %>